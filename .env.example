# Legacy AI Settings
VITE_LEGACY_AI_MODE=groq  # Options: groq, claude, local
VITE_ENABLE_LEARNING=true
VITE_ENABLE_PROACTIVE_ALERTS=true

# Optional: For better error tracking
VITE_APP_ENV=development