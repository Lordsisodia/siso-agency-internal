{
  "framework": "vite",
  "buildCommand": "npm run build",
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    },
    "api/*/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "PRISMA_CLI_QUERY_ENGINE_TYPE": "binary",
    "PRISMA_CLIENT_ENGINE_TYPE": "binary"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
